@font-face {
    font-family: 'Brandon';
    src: url('{{ "brandon_reg-webfont.eot" | asset_url }}');
    src: url('{{ "brandon_reg-webfont.eot?#iefix" | asset_url }}') format('embedded-opentype'),
         url('{{ "brandon_reg-webfont.woff" | asset_url }}') format('woff'),
         url('{{ "brandon_reg-webfont.ttf" | asset_url }}') format('truetype'),
         url('{{ "brandon_reg-webfont.svg#brandon" | asset_url }}') format('svg');
    font-weight: normal;
    font-style: normal;
}
