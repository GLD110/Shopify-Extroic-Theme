  text-align:right;
  height:40px;
  z-index: 99990;
  background-color: $nocolor;
  transition: all 0.6s ease 0s;
}
.side-nav-container-right .side-nav-right {
	min-height: 100%;
	top: 0px;
	max-height: 100%;
	overflow: auto;
	height: 100%;
	position: fixed;
	background-color: #FFF;
	max-width: 250px;
  	right: 0!important;
  	z-index: 99990!important;
}
.box-right-menu {
	border: 1px solid #bdbdbd;
	background-color: #f5f5f5;
	position: fixed;
	top: 0;
	bottom: 0;
	width: 400px;
	right: 0;
}
