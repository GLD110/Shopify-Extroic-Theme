//my col

// Common styles for small and large grid columns
.col-xs-2-4,
.col-xs-4-8,
.col-xs-7-2,
.col-xs-9-6,

.col-sm-2-4,
.col-sm-4-8,
.col-sm-7-2,
.col-sm-9-6,

.col-md-2-4,
.col-md-4-8,
.col-md-7-2,
.col-md-9-6,

.col-lg-2-4,
.col-lg-4-8,
.col-lg-7-2,
.col-lg-9-6
{
