{% if settings.home_blockhtml_enable %}
<div class="box">
  <div class="row">
    <div class="col-lg-12 col-md-12 hidden-xs hidden-sm">
      <div class="col-inner">
        <div class="banner-wrapper">
          <a class="ap-banner" href="{{ settings.home_blockhtml_link_1 }}">
            {{ 'home_html_1.png' | asset_url | img_tag: settings.home_blockhtml_tilte_1, "img-responsive" }}
          </a>
        </div>
        <div class="banner-wrapper">
          <a class="ap-banner" href="{{ settings.home_blockhtml_link_2 }}">
            {{ 'home_html_2.png' | asset_url | img_tag: settings.home_blockhtml_tilte_2, "img-responsive" }}
          </a>
        </div>
        <div class="banner-wrapper">
          <a class="ap-banner" href="{{ settings.home_blockhtml_link_3 }}">
            {{ 'home_html_3.png' | asset_url | img_tag: settings.home_blockhtml_tilte_3, "img-responsive" }}
          </a>
        </div>
      </div>
    </div>
  </div>
</div>
{% endif %}